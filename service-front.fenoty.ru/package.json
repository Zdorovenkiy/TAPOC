{
  "name": "tapac",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "imports": {
    "@*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*.js",
      "./src/*.jsx",
      "./src/*/index.ts",
      "./src/*/index.tsx",
      "./src/*/index.js",
      "./src/*/index.jsx"
    ]
  },
  "scripts": {
    "dev": "vite --host --mode dev",
    "serve": "vite build --watch --mode dev",

    "dev-prod": "vite --host --mode prod",
    "serve-prod": "vite build --watch --mode prod",


    "build": "run-p type-check \"build-only {@}\" --mode prod",
    "preview": "vite preview",
    "build-only": "vite build --mode prod",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "pinia": "^2.1.7",
    "swiper": "^11.1.4",
    "telegram-webapps-types": "^1.0.5",
    "vue": "^3.4.21",
    "vue-axios": "^3.5.2",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/tsconfig": "^0.5.1",
    "npm-run-all2": "^6.1.2",
    "sass": "^1.77.4",
    "telegram-webapps": "^7.2.0",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vite-plugin-vue-devtools": "^7.0.25",
    "vite-tsconfig-paths": "^4.3.2",
    "vue-tsc": "^2.0.11"
  }
}
